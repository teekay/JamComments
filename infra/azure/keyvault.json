            "name": "[parameters('vaults_jamcomments_staging_kv_name')]",
            "location": "eastus",
            "properties": {
                "sku": {
                    "family": "A",
                    "name": "Standard"
                },
                "tenantId": "9091203f-b96c-4d42-b3e6-2a86b61fb632",
                "accessPolicies": [
                    {
                        "tenantId": "9091203f-b96c-4d42-b3e6-2a86b61fb632",
                        "objectId": "96075eb4-dd71-4bc1-9a52-5c88cf686241",
                        "permissions": {
                            "keys": [
                                "Get",
                                "List",
                                "Update",
                                "Create",
                                "Import",
                                "Delete",
                                "Recover",
                                "Backup",
                                "Restore",
                                "GetRotationPolicy",
                                "SetRotationPolicy",
                                "Rotate"
